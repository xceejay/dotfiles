nmcli dev wifi connect $(nmcli dev wifi|sed '2,30 s/ /  /g'|rofi -dmenu -p         "connect to wifi🐸:"|awk '{print $1}') && sleep 10

if [ $(nmcli|grep -o -w "connected") == "connected" ]
    then 
    notify-send -t 1000 "connected🎯"
    else
        notify-send -t 1000 "disconnected...🐳"
        fi
